yelure\n\nlet Distance_to_player=0.65 // How far the object is relative to the player (negative will be behind)\nlet up_down_constant= -0.35  // Positive object will go higher, negative will go lower. eg: -0.65 places the object where your hands are, approximately. 0 will place the object right above your eyes.\nlet refresh_rate = 15      // Number of milliseconds. Please avoid a refresh rate < 30 as it could crash the grid.\n\nlet clone = false; // change this to false if you want to not clone the object.\n\n//-------------- Do not touch ---------------------------\n\nfunction moveObject(object,player){ // calculates new position of object\n\n  let spherePos = object.position;\n  let playerRotation = player.rotation;\n\n  let xDelta = Distance_to_player*Math.cos(-playerRotation.y+Math.PI/2 + Math.PI);\n  let yDelta = Distance_to_player*Math.tan(playerRotation.x);\n  let zDelta = Distance_to_player*Math.sin(-playerRotation.y+Math.PI/2 + Math.PI);\n\n      var positionRell = [\n    player.position.x - xDelta,\n    player.position.y - yDelta +(up_down_constant),\n    player.position.z - zDelta\n  ];\n\nreturn positionRell\n}\n\n\n\nlet timer;\n\nfunction setPosition(newobject,e){ // refreshes the positions\n    newobject.set({position:moveObject(newobject,e.player)})\n  if(!newobject.position || !newobject.rotation){\n    parcel.removeFeature(newobject)\n  }else{\n      \n  timer = setTimeout(()=>{\n    setPosition(newobject,e)\n  },refresh_rate)\n      \n  }\n}\n\nfeature.on('click',e=>{ // On click, create new object and start refreshing the position\nvar newobject = feature\n  \n//if(clone){\n//newobject = parcel.createFeature('vox-model')\n//newobject.set({scale:[feature.scale.x,feature.scale.y,feature.scale.z]})\n\n//newobject.set({url:feature._content.url})\n//newobject.position=feature.position\n//newobject.rotation=feature.rotation\n//}\n\nsetPosition(newobject,e)\n\n})\n\n\nlet cake = parcel.getFeatureById('cake')\n\ncake.on('click',e=>{\nconsole.log(\"clicked cake\")\n\nclearTimeout(timer);\n\n//console.log(e)\n\n\n  \n  \n})\n\n\n\n\n\n\n","version":"5.45.0","position":[-10.05061597604789,1.7634658125273064,2.972645664396899],"rotation":[0,1.5707963267948966,0],"animation":{"keyframes":[]},"collidable":false},{"id":"cake","url":"https://wiki.cryptovoxels.com/cakee.vox","type":"vox-model","uuid":"db9d054c-1ec7-4758-8597-0f9c0a93e866","flipX":true,"scale":[0.5,0.5,0.5],"script":"","version":"5.32.3","position":[-10,1.75,3.5],"rotation":[0,1.5707963267948966,0],"animation":{"keyframes":[]}},{"url":"https://siasky.net/KAAf8CwdD0Bz7-UES0qgEpm5slqvXGoIrBFlZ4TjskdhgQ","link":"","type":"vox-model","uuid":"f4c57457-a707-4113-a8cf-5b564a608d7b","flipX":true,"scale":[5,5,5],"script":"","version":"5.45.0","position":[-0.75,1.7,5.15],"rotation":[0,0,0],"animation":{"keyframes":[]}},{"id":"cat","url":"https://wiki.cryptovoxels.com/cat.vox","type":"vox-model","uuid":"ecc3aff7-b306-4e96-8128-3860b3a1fff9","flipX":true,"scale":[1,1,1],"script":"//parcel.on('playerenter', (e)=> {\n  //console.log(e.player.name,\" entered!\")\n//})\n\n//parcel.on('playerleave', (e)=> {\n  //console.log(e.player.name,\" left!\")\n//})\n\n\n","version":"5.51.4","position":[-6.5,1.75,3],"rotation":[0,0,0],"animation":{"keyframes":[]}}],"settings":{"sandbox":false,"hosted_scripts":false},"voxels":"","owner":"0x5AB52E17133a2D545b806a468b47650b077985Dc","lightmap_status":"None","parcel_users":null,"description":null,"name":"","label":null,"address":"10 Ghost Extension","suburb":"Punks","island":"Origin City","x1":179,"y1":0,"z1":-308,"x2":204,"y2":13,"z2":-298,"tileset":null,"palette":["#ffffff","#bfbfbf","#3b3b3b","#ff71ce","#01cdfe","#05ffa1","#b967ff","#fffb96"],"is_common":false,"visible":true,"kind":"plot"}}