or(Math.random()*1.5 + 0.1);\n    let deltaz = Math.floor(Math.random()*1.5 + 0.1);\n    deltax *= Math.floor(Math.random()*2) == 1 ? 1 : -1;\n    deltaz *= Math.floor(Math.random()*2) == 1 ? 1 : -1;\n\n    let deltay = Math.floor(Math.random()*5 + 15);\n\n    let angx = (Math.atan2(deltay, deltax)).toFixed(2);\n    let angy = (Math.atan2(deltaz, deltax)).toFixed(2);\n    let angz = (Math.atan2(deltay, deltaz)).toFixed(2);\n\n    console.log(deltax);\n    console.log(deltay);\n    console.log(deltaz);\n    console.log(angx);\n    console.log(angy)\n    console.log(angz);\n\n    let fw1 = firework1.createAnimation('position');\n    let fw2 = firework1.createAnimation('rotation');\n\n    fw1.setKeys([{\n      frame: 30, \n      value: firework1.position.add( new Vector3(deltax,deltay,deltaz))\n    }]);\n\n    fw2.setKeys([{\n      frame: 1,\n      value: firework1.rotation.add(new Vector3(Math.PI / -2 - angx, 0, Math.PI / -2 - angz))\n    }]);\n\n    firework1.startAnimations([fw1, fw2]);\n  }, 2000);\n});","position":[4.75,0.8,-0.25],"rotation":[0,1.5707963267948966,0]},{"id":"LjetL1","url":"https://media.giphy.com/media/3ohhwg2HbhMWrVXQXe/giphy.gif","type":"image","uuid":"45c12898-1007-49bf-a499-666a8abf8d5f","scale":[3.5,5.3,0],"script":"","uScale":1,"vScale":1,"stretch":true,"position":[3.5,4.25,-0.85],"rotation":[0,0.17,-1.05],"animation":{"keyframes":[]},"blendMode":"Screen","transparent":true,"updateDaily":false},{"id":"LjetR1","url":"https://media.giphy.com/media/3ohhwg2HbhMWrVXQXe/giphy.gif","type":"image","uuid":"f6ec7a87-8e9b-47de-8c5b-bf12c524c9f8","scale":[3.5,5.3,0],"script":"","uScale":1,"vScale":1,"stretch":true,"position":[-3.1,4.25,-0.85],"rotation":[0,0,1.05],"animation":{"keyframes":[]},"blendMode":"Screen","transparent":true,"updateDaily":false},{"id":"LjetR2","url":"https://media.giphy.com/media/3ohhwg2HbhMWrVXQXe/giphy.gif","type":"image","uuid":"93bd4217-3b71-475f-a82a-7fdd86ed03fb","scale":[4.9,6.1,0],"script":"","uScale":1,"vScale":1,"stretch