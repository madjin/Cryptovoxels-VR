42816433741889647/door.vox","type":"vox-model","uuid":"cf2e57b8-77ad-4199-84e5-bcf92e0c967f","flipX":true,"scale":[8,8,2],"script":"let switchgreen = parcel.getFeatureById('switchgreen');\n\nlet switchblue = parcel.getFeatureById('switchblue');\n\nlet switchred = parcel.getFeatureById('switchred');\n\nlet switchblanc = parcel.getFeatureById('switchblanc');\n\nlet door = parcel.getFeatureById('door');\n\nlet green = 0;\nlet blue = 0;\nlet red = 0;\n\n\nlet r1 = door.createAnimation('position');\n\nr1.setKeys([{\nframe: 60, \nvalue: door.position.add( new Vector3(4,0,0))\n}])\n\nfunction open() {\n\tif(green==1 && blue==1 && red==1){\n\t\tdoor.startAnimations( [r1] );\n\t\tblue=0;\n\t\tgreen=0;\n\t\tred=0;\n\t}\n}\n\nswitchgreen.on('click', (e) => {\n\tif(green==1){\n\t\tblue=0;\n\t\tred=0;\n\t\tgreen=0;\n\t}else{\n\t\tgreen=1;\n\t}\n})\n\nswitchblue.on('click', (e) => {\n\tif(green==1){\n\t\tblue = 1;\n\t}else{\n\t\tblue = 0;\n\t\tred = 0;\n\t\tgreen = 0;\n\t}\n\t\n})\n\nswitchred.on('click', (e) => {\n\tif(green==1 && blue==1){\n\t\tred=1;\n\t}else{\n\t green = 0;\n\t red = 0;\n\t blue = 0;\n\t}\n\t\n\tsetTimeout(open(), 500);\n\n\t\n})\n\nswitchblanc.on('click', (e) => {\n\tdoor.startAnimations( [r1] );\n})","position":[-0.4,0.75,-5.05],"rotation":[0,0,0],"animation":{"keyframes":[{"frame":0,"value":[0,0,0]}],"destination":"position"},"collidable":true},{"id":"switchgreen","text":"A","type":"button","uuid":"78a09e19-dfa5-46cd-b271-689de871d10b","color":"green","scale":[0.5,0.5,0.5],"script":"","soundId":"0","position":[-3,2.25,-5.65],"rotation":[-1.57,0,0]},{"id":"switchblue","text":"A","type":"button","uuid":"ec9bc83a-9c87-4dc7-a721-69588d643574","color":"blue","scale":[0.5,0.5,0.5],"script":"","soundId":"0","position":[-3,1.75,-5.65],"rotation":[-1.57,0,0]},{"id":"switchblanc","text":"A","type":"button","uuid":"2bde719a-a83a-43d0-a537-9d678238b66f","color":"white","scale":[0.5,0.5,0.5],"script":"","position":[-3,2.25,-5.22],"rotation":[1.57,0,0]},{"url":"https://