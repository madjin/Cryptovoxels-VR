e,"scale":[2.7,1.9,2.3],"script":"","version":"8.13.1","position":[-0.6,4.6,4.1],"rotation":[0,1.5708,0],"animation":{"keyframes":[]},"cubescale":true,"collidable":true},{"id":"sidestairs9","url":"https://www.dropbox.com/s/vh0wat08ogcsc7w/StoneStairs1-4T.vox?dl=0","type":"vox-model","uuid":"4fe55f0d-5829-410f-840d-618c00ad62d6","flipX":true,"scale":[2.7,1.9,2.3],"script":"","version":"8.13.1","position":[-0.6,4.6,5.45],"rotation":[0,1.5708,0],"animation":{"keyframes":[]},"cubescale":true,"collidable":true},{"id":"","url":"https://www.dropbox.com/s/0xlsnj4splt64fz/Smoke.png?dl=0","type":"particles","uuid":"769dfac9-bf1d-4969-8367-a925542c6de6","scale":[1,1,1],"maxSize":0.9,"minSize":0.78,"emitRate":6,"position":[2.6,1.55,8.1],"rotation":[1.22,0.87,-0.35],"animation":{"keyframes":[{"frame":0,"value":[1.25,0.5,0.5]},{"frame":33,"value":[1,1,1.5]},{"frame":81,"value":[1.25,0.5,2.25]}],"destination":"scaling"}},{"url":"https://www.dropbox.com/s/r6tbuh7qalb62ts/Smoke3.png?dl=0","type":"particles","uuid":"52ed05ab-03c7-43fb-8fcc-1fbce5364f99","scale":[1,1,1],"color1":"#0d0d0d","color2":"#ffffff","maxSize":0.54,"minSize":0.42,"emitRate":41,"position":[2.7,1.3,7.9],"rotation":[1.57,1.57,0],"animation":{"keyframes":[]}},{"url":"https://www.dropbox.com/s/j3lt3r87a1k7ozz/light%20long.png?dl=0","type":"image","uuid":"907add46-814a-4811-987b-60fe2b5f1e71","scale":[0.1,1.8,0],"stretch":true,"position":[-2.84,3.65,-1.7],"rotation":[-1.57,3.14,0],"animation":{"keyframes":[]},"blendMode":"Screen","transparent":false,"updateDaily":false},{"url":"https://www.dropbox.com/s/j3lt3r87a1k7ozz/light%20long.png?dl=0","type":"image","uuid":"0df51a54-21d1-49e2-bce9-89247490dd17","scale":[1.1,2.9,0.9],"stretch":true,"position":[-2.8,3.7,-1.7],"rotation":[1.57,6.28,0],"animation":{"keyframes":[]},"blendMode":"Combine","transparent":true,"updateDaily":false},{"url":"https://www.dropbox.com/s/j3lt3r87a1k7ozz/light%20long.png?dl=0","type":"image","uuid":"d5ca972a-9c4c-41c2-b157-26bdcb4d0ffd","scale":[0.7,2.1,0.9],"stretch":true,"position":[-4.95,3.7,-1.7],"rotation":[1.57,4.71,0],"animation":{"keyframes":[]},"blendMode":"Combine","transparent":true,"updateDaily":false},{"url":"https://www.dropbox.com/s/j3lt3r87a1k7ozz/light%20long.png?dl=0","type":"image","uuid":"76077ee5-d0a6-4958-862e-ed491e0cb25a","scale":[0.1,1.8,0],"stretch":true,"position":[-4.9,3.65,-1.7],"rotation":[-1.57,6.28,1.57],"animation":{"keyframes":[]},"blendMode":"Screen","transparent":false,"updateDaily":false},{"id":"game 1","type":"vid-screen","uuid":"ccccb6a1-ef1e-4b2b-aae2-be61c1debf0e","scale":[0.8,0.9,0],"script":"let ball, paddle, frame, blocks\n\n// Canvas Size 64*64\nlet PIXELS = 64;\n\n// Each pixel has a Red, Green & Blue sequential pixel\nlet RGB_PIXEL_SIZE = 3;\n\n// Block Grid\nlet BLOCK_ROWS = 5;\nlet BLOCK_COLS = 8;\n\nclass Color {\n  constructor(r, g, b) {\n    this.pixels = [r, g, b];\n  }\n}\n\n// COLORS\nlet RED = new Color(255, 0, 0);\nlet GREEN = new Color(0, 255, 0);\nlet BLUE = new Color(128, 128, 255);\nlet WHITE = new Color(255, 255, 255);\nlet GRAY = new Color(128, 128, 128);\n\n\n// Listeners\nfeature.on('keys', e => {\n  if (e.keys.left) {\n    paddle.moveLeft();\n  } else if (e.keys.right) {\n    paddle.moveRight();\n  }\n})\n\nfeature.on('start', e => {\n  reset();\n})\n\nfeature.on('frame', e => {\n  frame += 1;\n  update();\n  draw();\n})\n\n// Update/Draw/Reset\n\nfunction update() {\n  ball.update();\n}\n\nfunction draw() {\n  feature.screen.fill(GRAY);\n  \n  ball.draw();\n  paddle.draw();\n  for (j = 0; j < blocks.length; j ++) {\n    blocks[j].draw();\n  }\n}\n\nfunction reset() {\n  frame = 0;\n\n  // Ball\n  let randomX = Math.round(Math.random() * PIXELS);\n  ball = new Ball(new ColoredBlock(randomX, 32, 1, 1, GREEN));\n\n  // Paddle\n  paddle = new Paddle(new ColoredBlock(29, 60, 10, 1, BLUE));\n\n  // Blocks\n  blocks = []\n  for (let row = 0; row < BLOCK_ROWS; row++) {\n    for (let j = 0; j < BLOCK_COLS; j ++) {\n      let xPos = Math.floor(feature.screenWidth / 8 * j + 2);\n      let yPos = row * 4 + 2;\n      blocks.push(new Square(xPos, yPos, 5, 3, RED));\n    }\n  }\n\n  // Draw\n  draw()\n}\n\n// Classes\n\nclass Ball {\n  constructor(obj) {\n    this.object = obj;\n    \n    this.xVel = Math.random() > 0.5 ? 1 : -1;\n    this.yVel = 1;\n  }\n  \n  update() {\n    if (frame % 2 != 0) {\n      return\n    }\n    \n    // wall collision detection\n    if ((this.object.x + this.xVel) >= PIXELS \n      || (this.object.x + this.xVel) < 0) {\n      this.xVel *= -1;\n    }\n    if ((this.object.y + this.yVel) < 0) {\n      this.yVel *= -1;\n    }\n    if ((this.object.y + this.yVel) >= PIXELS) {\n      reset();\n    }\n    \n    // paddle collision detection\n    if ((this.object.y + this.yVel) == paddle.object.y) {\n      let nextX = this.object.x + this.xVel;\n      if (nextX >= paddle.object.x \n        && nextX <= paddle.object.x + paddle.object.width) {\n        this.yVel *= -1;\n      }\n    }\n    \n    // block collision detection\n    for (let i = 0; i < blocks.length; i++) {\n      let block = blocks[i]\n      let nextX = this.object.x + this.xVel\n      let nextY = this.object.y + this.yVel\n      \n      if (nextX >= block.x \n        && nextX < block.x + block.width \n        && nextY >= block.y \n        && nextY < block.y + block.height) {\n        if (this.object.x < block.x \n          || this.object.x > block.x + block.width - 1) {\n          this.xVel *= -1;\n        }\n        if (this.object.y < block.y \n          || this.object.y > block.y + block.height - 1) {\n          this.yVel *= -1;\n        }\n        \n        blocks.splice(i, 1)\n        break\n      }\n    }\n    \n    // update position\n        \n    this.object.x += this.xVel\n    this.object.y += this.yVel\n  }\n  \n  draw() {\n    this.object.draw();\n  }\n}\n\nclass Paddle {\n  constructor(obj) {\n    this.object = obj;\n  }\n  \n  draw() {\n    this.object.draw();\n  }\n  \n  moveLeft() {\n    if (this.object.x > 0) {\n      this.object.x -= 2;\n      draw();\n    }\n  }\n  \n  moveRight() {\n    if (this.object.x + this.object.width <= PIXELS) {\n      this.object.x += 2;\n      draw();\n    }\n  }\n}\n\nclass Square {\n  constructor(x, y, w, h) {\n    this.x = x;\n    this.y = y;\n    this.width = w;\n    this.height = h;\n    this.objects = [\n      // Left Line\n      new ColoredBlock(x, y, 1, h, RED),\n      // Right Line\n      new ColoredBlock((x + w - 1), y, 1, h, RED),\n      // Top Line\n      new ColoredBlock(x, y, w, 1, RED),\n      // Bottom Line\n      new ColoredBlock(x, (y + h - 1), w, 1, RED)\n    ];\n  }\n  \n  draw() {\n    this.objects.forEach((obj) => {\n      obj.draw();\n    })\n  }\n}\n\nclass ColoredBlock {\n  constructor(x, y, w, h, c) {\n    this.x = x;\n    this.y = y;\n    this.width = w;\n    this.height = h;\n    this.color = c;\n  }\n  \n  draw() {\n    let screen_width_in_rgb_pixels = feature.screenWidth * RGB_PIXEL_SIZE;\n    for (let i = 0; i < this.width; i++) {\n      for (let j = 0; j < this.height; j++) {\n        for(let rgbOffset = 0; rgbOffset < this.color.pixels.length; rgbOffset++){\n          let xPos = (this.x + i) * RGB_PIXEL_SIZE + rgbOffset;\n          let yPos = (this.y + j) * screen_width_in_rgb_pixels;\n          feature.screen[xPos + yPos] = this.color.pixels[rgbOffset];\n        }\n      }\n    }\n  }\n}","version":"22378","position":[-2.9,2.2,-3.45],"rotation":[0.7854,3.14159,0]},{"url":"https://www.dropbox.com/s/kyr0nt9f058zyc6/White%20label%20Arcade-0.vox?dl=0","type":"vox-model","uuid":"c4e84f66-d69c-4879-b1ab-732825876bf0","flipX":true,"scale":[2.2,2.2,2.2],"script":"","position":[-2.9,0.75,-3.5],"rotation":[0,1.5707963267948966,0],"animation":{"keyframes":[]},"cubescale":true,"collidable":true},{"id":"doomsday","url":"https://www.dropbox.com/s/4zc68ko4e46ur0u/Bunker%20sign.png?dl=0","link":"https://www.doomsdaynft.io/map","type":"image","uuid":"ffee0ff5-dbc5-4bca-a7e8-0d417938e54c","scale":[0.8,0.2,0],"uScale":1,"vScale":1,"opacity":"1.00","stretch":true,"version":"16458","position":[-0.45,2.8,-1.4],"rotation":[0.17453,1.74533,0],"animation":{"keyframes":[],"destination":null},"blendMode":"Combine","transparent":false,"updateDaily":false},{"type":"spawn-point","uuid":"5ea8f4b6-8b97-441c-b143-7d438c50a3ff","scale":[1,1,1],"script":"","version":"5.9.0","position":[-5.4,0.75,-0.45],"rotation":[0,1.5707963267948966,0]}],"scripting":false,"brightness":1.7},"settings":{"sandbox":false,"hosted_scripts":false},"island":"Origin City","kind":"plot","minted":true}}