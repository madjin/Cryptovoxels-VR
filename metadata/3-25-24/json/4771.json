atureId(name, number) {\n  return name + number.toString().padStart(2,'0');\n}\n\nfunction rainbow(numOfSteps, step) {\n    // based on http://stackoverflow.com/a/7419630\n    // This function generates vibrant, \"evenly spaced\" colours (i.e. no clustering). This is ideal for creating easily distiguishable vibrant markers in Google Maps and other apps.\n    // Adam Cole, 2011-Sept-14\n    // HSV to RBG adapted from: http://mjijackson.com/2008/02/rgb-to-hsl-and-rgb-to-hsv-color-model-conversion-algorithms-in-javascript\n    var r, g, b;\n    var h = step / numOfSteps;\n    var i = ~~(h * 6);\n    var f = h * 6 - i;\n    var q = 1 - f;\n    switch(i % 6){\n        case 0: r = 1, g = f, b = 0; break;\n        case 1: r = q, g = 1, b = 0; break;\n        case 2: r = 0, g = 1, b = f; break;\n        case 3: r = 0, g = q, b = 1; break;\n        case 4: r = f, g = 0, b = 1; break;\n        case 5: r = 1, g = 0, b = q; break;\n    }\n\n    var c = \"#\" + (\"00\" + (~ ~(r * 235)).toString(16)).slice(-2) + (\"00\" + (~ ~(g * 235)).toString(16)).slice(-2) + (\"00\" + (~ ~(b * 235)).toString(16)).slice(-2);\n    return (c);\n}\n","version":"40113","position":[4.3,2.45,5.25],"rotation":[0,-4.71239,0]},{"url":"","type":"cube","uuid":"1d044206-1577-42ba-83f5-50bc6f4e44c3","scale":[0.5,0.1,0.5],"version":"6.9.0","position":[0.1,2.75,-0.8],"rotation":[0,0,0]},{"url":"https://www.dropbox.com/s/1synmq85j4fj08f/Monolith.126.final.upside.vox?dl=0","type":"megavox","uuid":"bae20fc1-44c6-4c40-bc3a-715871e0bfa0","flipX":true,"scale":[6.6,6.6,10],"script":"","version":"7.13.0","position":[-0.42,32.65,-8.18],"rotation":[1.57,0,0],"animation":{"keyframes":[]}},{"url":"https://www.dropbox.com/s/2rvmrysdv2wtmeo/Monolith.126.final.downside.floor.vox","type":"megavox","uuid":"41374323-d042-4268-8475-3f1936cb7d6d","flipX":true,"scale":[6.6,6.6,10],"script":"","version":"6.9.3","position":[-0.42,12.9,8.45],"rotation":[-1.57,0,0],"animation":{"keyframes":[]},"collidable":true},{"id":"rainbow00","u